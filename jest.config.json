{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".test.ts$",
  "moduleDirectories": [
    "node_modules",
    "<rootDir>"
  ],
  "moduleNameMapper": {
    "~/(.*)": "<rootDir>/src/$1",
    "@domain-model/(.*)": [
      "<rootDir>/src/domain/model/$1"
    ]
  },
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": [
    "**/*.(t|j)s"
  ],
  "coverageDirectory": "<rootDir>/coverage"
}